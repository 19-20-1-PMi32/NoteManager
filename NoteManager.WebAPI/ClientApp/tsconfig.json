{
  "compilerOptions": {
    "allowJs": true,
    "sourceMap": true,
    "jsx": "react",
    "lib": [ "dom", "es6" ],
    "module": "es6",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "outDir": "./dist",
    "target": "es5",
    "typeRoots": ["./src/GlobalTypes", "./node_modules/@types"]
  },
  "include": ["./src/**/*"],
  "exclude": ["node_modules", "dist"]
}
